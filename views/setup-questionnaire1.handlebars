<form action="/setup-questionnaire/add" method="POST">
  <h1>Create questionnaire</h1>
  <input type="text" name="questionnaire_name" required>
  <input type="submit">
</form>




<form action="/setup-questionnaire/{{something_id}}/add" method="POST">
           <h1>Setup Questionnaire</h1>
           <input type="text" name="question_description" required>
           <input type="submit">
</form>
<div class="container">

    <table>
    {{#each questions}}
        <tr>
            <td>{{question_description}}</td>
                <td><a href="/setup-questionnaire/edit/{{question_id}}">edit</a></td>
                <td><a href="/setup-questionnaire/delete/{{question_id}}">delete</a></td>
        </tr>
    {{/each}}
    <p>

    </p>

</div>
