<div id="setupQuestionStep3">
    <div class="container">
        <form action="/setup-questionnaire-step-3/add" method="POST">
            <h1>Setup Questionnaire - Step 3</h1>
            <h3>Invite Users to answer questionnaires:</h3>
            <input type="email" name="invitations_email" required/>
            {{#each questionnaire}}
                <input type="hidden" name="questionnaire_id" value="{{questionnaire_id}}" />
            {{/each}}
            <input type="submit" />
        </form>

        <table>
        {{#each invitations}}
            <tr>
                <td>{{invitations_email}}</td>
            </tr>
        {{/each}}
        </table>

        <a href="/dashboard">Finnish</a>
    </div>
</div>